# start fresh
clear
PATH=""

if [ -x /usr/libexec/path_helper ]; then
    eval `/usr/libexec/path_helper -s`
fi

if [ -f $HOME/.bash_aliases ]; then
    . $HOME/.bash_aliases

fi
#export PATH=$PATH:/usr/local/sbin:/opt/homebrew/bin
export PATH=:/opt/homebrew/bin:/opt/homebrew/sbin:$PATH
export QUANTIC_DEV_DIR=~/code/back_royal
export NGROK_SUBDOMAIN=johnmpedago

test -f $QUANTIC_DEV_DIR/bash/back-royal.bash && source $QUANTIC_DEV_DIR/bash/back-royal.bash
if [ -x "$(command -v asdf)" ]; then
    . /opt/homebrew/opt/asdf/libexec/asdf.sh
    . /opt/homebrew/opt/asdf/etc/bash_completion.d/asdf.bash
fi
